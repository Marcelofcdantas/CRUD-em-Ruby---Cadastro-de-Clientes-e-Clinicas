<p id="notice"><%= notice %></p>

<h1>Clinicas</h1>

<table>
  <thead>
    <tr>
      <th>Razao social</th>
      <th>Nome fantasia</th>
      <th>Cnpj</th>
      <th>Rua</th>
      <th>Numero</th>
      <th>Complemento</th>
      <th>Bairro</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th>Cep</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clinicas.each do |clinica| %>
      <tr>
        <td><%= clinica.razao_social %></td>
        <td><%= clinica.nome_fantasia %></td>
        <td><%= clinica.cnpj %></td>
        <td><%= clinica.rua %></td>
        <td><%= clinica.numero %></td>
        <td><%= clinica.complemento %></td>
        <td><%= clinica.bairro %></td>
        <td><%= clinica.cidade %></td>
        <td><%= clinica.estado %></td>
        <td><%= clinica.cep %></td>
        <td><%= link_to 'Show', clinica %></td>
        <td><%= link_to 'Edit', edit_clinica_path(clinica) %></td>
        <td><%= link_to 'Destroy', clinica, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clinica', new_clinica_path %>
